		<script>// Close Cookies Warning
function showCookieConsent() {
  var element = document.querySelector('.cookies');
  var opacity = 1; // initial opacity
  var timer = setInterval(function () {
    if (opacity <= 0.1) {
      clearInterval(timer);
      element.style.display = 'none';
    }
    element.style.opacity = opacity;
    opacity -= opacity * 0.1;
  }, 10);
}

document
  .querySelector('.cookies--close')
  .addEventListener('click', function () {
    // Set the "_cookies" value to 'valid'.
    localStorage.setItem('_cookies', 'valid');

    // Show the cookie consent again tomorrow.
    var tomorrow = new Date();
    tomorrow.setDate(tomorrow.getDate() + 1);
    localStorage.setItem('_cookieConsentExpiry', tomorrow.toISOString());

    showCookieConsent();
  });

// Check if "_cookies" is set and has not expired.
var cookieConsentExpiry = localStorage.getItem('_cookieConsentExpiry');
if (
  !localStorage.getItem('_cookies') ||
  (cookieConsentExpiry && new Date() < new Date(cookieConsentExpiry))
) {
  showCookieConsent();
}

